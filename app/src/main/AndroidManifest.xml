<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.chy.swipemenuandpulltorefresh">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <!--ExpandListView-->
        <activity android:name=".ExpandViewActivity.RExpandListViewActivity"/>
        <activity android:name=".ExpandViewActivity.SMExpandListViewActivity"/>
        <activity android:name=".ExpandViewActivity.SMRExpandListViewActivity"/>
        <activity android:name=".ExpandViewActivity.SMExpandListViewActivity2"/>
        <activity android:name=".ExpandViewActivity.SMRExpandListViewActivity2"/>

        <!--ListView-->
        <activity android:name=".ListViewAcitvity.RListViewActivity"/>
        <activity android:name=".ListViewAcitvity.SMListViewActivity"/>
        <activity android:name=".ListViewAcitvity.SMRListViewActivity"/>
        <activity android:name=".ListViewAcitvity.SMListViewActivity2"/>
        <activity android:name=".ListViewAcitvity.SMRListViewActivity2"/>

    </application>

</manifest>